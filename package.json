{
  "version": "1.0.25-4",
  "name": "@findable-no/findable-components",
  "author": "Findable",
  "description": "Findable components",
  "keywords": [
    "findable",
    "components",
    "react",
    "treeview",
    "react-window",
    "virtualization",
    "optimized",
    "folders"
  ],
  "license": "Apache 2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.15.1 < 16.19.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "prepare": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/findable-no/findable-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findable-no/findable-components.git"
  },
  "bugs": {
    "url": "https://github.com/findable-no/findable-components/issues"
  },
  "module": "dist/findable-components-tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/findable-components-tsdx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/findable-components-tsdx.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-window": "^1.8.5",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.10.0",
    "@mantine/hooks": "^5.10.0",
    "ag-grid-community": "^28.2.1",
    "ag-grid-react": "^28.2.1",
    "memoize-one": "^6.0.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.7.1",
    "react-window": "^1.8.8",
    "usehooks-ts": "^2.9.1"
  }
}
